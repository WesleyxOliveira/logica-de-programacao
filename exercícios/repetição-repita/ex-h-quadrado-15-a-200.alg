algoritmo "semnome"
// Função :
// Autor :
// Data : 16/10/2021
// Seção de Declarações 
var
   Contador: inteiro
   Resultado: real
inicio
      Contador <- 15
      Repita
            Resultado <- Contador^2
            Escreval (Contador, "^2 = ", Resultado)
            Contador <- Contador +1
      Ate (Contador > 200)
fimalgoritmo
