algoritmo "semnome"
// Função :
// Autor :
// Data : 03/10/2021
// Seção de Declarações 
var
   Contador: inteiro
inicio
      Contador <- 0
      Enquanto (Contador <= 20) faca
      Se (Contador mod 2 <> 0) entao
               Escreval (Contador)
      FimSe
           Contador <- Contador + 1
      FimEnquanto
fimalgoritmo
