algoritmo "semnome"
// Função :
// Autor :
// Data : 01/10/2021
// Seção de Declarações 
var
   Contador, SOMA: inteiro
inicio
      Contador <- 2
      SOMA <- 0
      Enquanto (Contador <= 10) faca
               Escreval (Contador)
               SOMA <- SOMA + Contador
               Contador <- Contador + 2
      FimEnquanto
      Escreva ("RESULTADO: ", SOMA)
fimalgoritmo
