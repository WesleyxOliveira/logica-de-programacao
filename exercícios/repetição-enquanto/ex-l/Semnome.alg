algoritmo "semnome"
// Função :
// Autor :
// Data : 05/10/2021
// Seção de Declarações 
var
   numero_final, N: inteiro
inicio
      Escreva ("Digite um número inteiro: ")
      Leia (N)
      numero_final <- 1
      Enquanto (N >=1) faca
               numero_final <- n* numero_final
               N <- N -1
      FimEnquanto
      Escreva (N, "! = ", numero_final)
fimalgoritmo
