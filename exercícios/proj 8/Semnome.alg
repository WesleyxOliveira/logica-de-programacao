algoritmo "semnome"
// Função :
// Autor :
// Data : 14/09/2021
// Seção de Declarações 
var
A, B, C, D, Resultado: inteiro

inicio
      Escreva ("Informe um valor para A: ")
      Leia (A)
      Escreva ("Informe um valor para B ")
      Leia (B)
      C <- B
      D <- A
      Escreva ("Valor de A: ", C, " Valor de B: ", D)
fimalgoritmo
