algoritmo "semnome"
// Função :
// Autor :
// Data : 15/09/2021
// Seção de Declarações 
var
   N1, N2, S, S2: real
inicio
      Escreva ("Informe um número: ")
      Leia (N1)
      Escreva ("Informe outro número: ")
      Leia (N2)
      S <- N1-N2
      S2 <- S^2
      Escreva ("O resultado da subtração entre N1 e N2 ao quadrado é: ", S2)
fimalgoritmo
