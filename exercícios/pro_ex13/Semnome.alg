algoritmo "semnome"
// Função :
// Autor :
// Data : 16/09/2021
// Seção de Declarações 
var
   R, C, D: real
inicio
      Escreva ("Informe o valor em reais: ")
      Leia (R)
      Escreva ("Informe o valor da cotação: ")
      Leia (C)
      D <- R/C
      Escreva ("O valor convertido para dólar é: ", D:4:2)
      
fimalgoritmo
