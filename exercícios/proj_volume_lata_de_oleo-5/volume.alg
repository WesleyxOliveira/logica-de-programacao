algoritmo "Volume_lata"
// Função :
// Autor :
// Data : 13/09/2021
// Seção de Declarações 
var
R, A, V: real

inicio
   Escreva ("Informe o raio da lata: ")
   Leia (R)
   Escreva ("Informe a altura da lata: ")
   Leia (A)
   V <- 3.14159*R^2*A
   Escreva ("O volume da lata é: ", V, " metros cubicos")

fimalgoritmo
