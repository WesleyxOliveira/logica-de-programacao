algoritmo "semnome"
// Função :
// Autor :
// Data : 29/10/2021
// Seção de Declarações 
var
   Cont, V: inteiro
inicio
      Escreva ("Digite um valor: ")
      Leia (V)
      Se (V % 2 <> 0) entao
         V <- V -1
      FimSe
      Para Cont <- V ate 0 passo -2 faca
           Escreval (Cont)
      FimPara
fimalgoritmo
